      \'field_prices_information\' => array(
        \'und\' => array(
          array(
            \'value\' => "<h2>Admission Prices for 2015</h2>\\r\\n\\r\\n<h3>Standard Admission Prices (orange, green and blue dates)</h3>\\r\\n\\r\\n<p>Adult: £5.50<br />\\r\\nChild (5-16): £3.50<br />\\r\\nConcession: £5.00<br />\\r\\nFamily Ticket (2 adults and up to 3 children): £16.00<br />\\r\\nUnder 5 years old free</p>\\r\\n\\r\\n<h3>Special Event Prices</h3>\\r\\n\\r\\n<p>Adult: £7.00<br />\\r\\nChild (5-16): £4.50<br />\\r\\nConcession: £6.00<br />\\r\\nFamily Ticket (2 adults and up to 3 children): £20.00<br />\\r\\nUnder 5 years old free</p>\\r\\n\\r\\n<h2>&nbsp;</h2>\\r\\n",
            \'format\' => \'filtered_html\',
            \'safe_value\' => "<h2>Admission Prices for 2015</h2>\\n<h3>Standard Admission Prices (orange, green and blue dates)</h3>\\n<p>Adult: £5.50<br />\\nChild (5-16): £3.50<br />\\nConcession: £5.00<br />\\nFamily Ticket (2 adults and up to 3 children): £16.00<br />\\nUnder 5 years old free</p>\\n<h3>Special Event Prices</h3>\\n<p>Adult: £7.00<br />\\nChild (5-16): £4.50<br />\\nConcession: £6.00<br />\\nFamily Ticket (2 adults and up to 3 children): £20.00<br />\\nUnder 5 years old free</p>\\n<h2> </h2>\\n",
