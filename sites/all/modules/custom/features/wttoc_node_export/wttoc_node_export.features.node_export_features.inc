      \'field_prices_information\' => array(
        \'und\' => array(
          array(
            \'value\' => "<h3>£3.00 OVER 1YR</h3>\\r\\n\\r\\n<h3>£0.50 UNDER 1YR</h3>\\r\\n",
            \'format\' => \'filtered_html\',
