      \'field_prices_information\' => array(
        \'und\' => array(
          array(
            \'value\' => "<h2><strong>Discovery Pass</strong></h2>\\r\\n\\r\\n<div>A £29 Discovery Pass will buy you daily parking for a year at no additional cost.</div>\\r\\n",
            \'format\' => \'filtered_html\',
            \'safe_value\' => "<h2><strong>Discovery Pass</strong></h2>\\n<p>A £29 Discovery Pass will buy you daily parking for a year at no additional cost.</p>\\n",
