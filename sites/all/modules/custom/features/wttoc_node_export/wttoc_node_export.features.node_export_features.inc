            \'uri\' => \'public://museum-of-east-anglian-life-suffolk-kids_0.jpg\',
            \'filemime\' => \'image/jpeg\',
            \'filesize\' => \'157728\',
            \'status\' => \'1\',
            \'timestamp\' => \'1446828632\',
            \'uuid\' => \'707a86b9-73b8-4056-898d-f08a9cbb17f7\',
            \'rdf_mapping\' => array(),
            \'alt\' => \'Photograph of two children playing with mini tractors at The Museum of East Anglian Life.\',
            \'title\' => \'Mini Tractors are always a popular choice\',
            \'width\' => \'800\',
            \'height\' => \'600\',
            \'node_export_file_path\' => \'public://museum-of-east-anglian-life-suffolk-kids_0.jpg\',
