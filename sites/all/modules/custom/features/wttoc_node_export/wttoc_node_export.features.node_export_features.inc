      \'field_prices_information\' => array(
        \'und\' => array(
          array(
            \'value\' => "<h2>Prices</h2>\\r\\n\\r\\n<p>Gorillas (18+): £31-33<br />\\r\\nBaboons (10-17 years): £25</p>\\r\\n",
            \'format\' => \'filtered_html\',
            \'safe_value\' => "<h2>Prices</h2>\\n<p>Gorillas (18+): £31-33<br />\\nBaboons (10-17 years): £25</p>\\n",
