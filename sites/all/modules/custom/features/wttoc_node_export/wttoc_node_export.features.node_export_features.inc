      \'field_prices_information\' => array(
        \'und\' => array(
          array(
            \'value\' => "<p><strong>Operating day:</strong>&nbsp;<strong>Adult</strong> £8, <strong>child</strong> £4 (up to&nbsp;16-years-old, under fives free), <strong>conc.</strong> £7, <strong>family</strong> £20.</p>\\r\\n\\r\\n<p><strong>Non-operating day:</strong>&nbsp;Free entry, but donations gratefully received. All tickets (except special event tickets) are season tickets valid for 12 months (excluding some special events)</p>\\r\\n",
            \'format\' => \'filtered_html\',
            \'safe_value\' => "<p><strong>Operating day:</strong> <strong>Adult</strong> £8, <strong>child</strong> £4 (up to 16-years-old, under fives free), <strong>conc.</strong> £7, <strong>family</strong> £20.</p>\\n<p><strong>Non-operating day:</strong> Free entry, but donations gratefully received. All tickets (except special event tickets) are season tickets valid for 12 months (excluding some special events)</p>\\n",
