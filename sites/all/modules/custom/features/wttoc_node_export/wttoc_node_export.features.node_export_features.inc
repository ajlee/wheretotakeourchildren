            \'uuid\' => \'ae02b869-1f89-4d5c-90fe-08f4527f89f0\',
            \'rdf_mapping\' => array(),
            \'alt\' => \'Man and boy sitting near a variety of Steampunk gadgets.\',
            \'title\' => \'Steampunk Items\',
