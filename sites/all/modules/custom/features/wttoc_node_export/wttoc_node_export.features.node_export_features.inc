      \'field_prices_information\' => array(
        \'und\' => array(
          array(
            \'value\' => "<h1>Prices</h1>\\r\\n\\r\\n<h2>2016 prices</h2>\\r\\n\\r\\n<h3>Prices applicable from&nbsp;19th March 2016 – 21st October 2016</h3>\\r\\n\\r\\n<div>Type Price Adult£9.00 Child£8.00 Over 65s £8.50 Family (2+2) £32.00</div>\\r\\n\\r\\n<p>This includes activities throughout the day (pony rides, train rides, barrel bug, meet a Suffolk Punch, hug a bunny and much more)</p>\\r\\n\\r\\n<h3><strong>Season tickets are available. Please ask for&nbsp;details&nbsp;on 01728 746475</strong></h3>\\r\\n\\r\\n<p>2016&nbsp;Season&nbsp;Ticket prices:</p>\\r\\n\\r\\n<p>Adult | £36.00</p>\\r\\n\\r\\n<p>Child | £32.00</p>\\r\\n\\r\\n<p>Senior | £34.00</p>\\r\\n\\r\\n<p>Season ticket holders are entitled to a 5%&nbsp;<a class=\\"pxInta\\" href=\\"http://www.eastonfarmpark.co.uk/prices/#\\" id=\\"PXLINK_4_0_3\\">discount</a>&nbsp;on purchases made in our Barmy Barn and&nbsp;<a class=\\"pxInta\\" href=\\"http://www.eastonfarmpark.co.uk/prices/#\\" id=\\"PXLINK_6_0_5\\">Shop</a>&nbsp;and 11 children for the price of 10 on our birthday party packages.</p>\\r\\n\\r\\n<p><strong>Note:</strong>&nbsp;A child ticket is for children that are 2 years old to 16 years old.</p>\\r\\n\\r\\n<p>Under 2s are free</p>\\r\\n",
            \'format\' => \'filtered_html\',
            \'safe_value\' => "<h1>Prices</h1>\\n<h2>2016 prices</h2>\\n<h3>Prices applicable from 19th March 2016 – 21st October 2016</h3>\\n<p>Type Price Adult£9.00 Child£8.00 Over 65s £8.50 Family (2+2) £32.00</p>\\n<p>This includes activities throughout the day (pony rides, train rides, barrel bug, meet a Suffolk Punch, hug a bunny and much more)</p>\\n<h3><strong>Season tickets are available. Please ask for details on 01728 746475</strong></h3>\\n<p>2016 Season Ticket prices:</p>\\n<p>Adult | £36.00</p>\\n<p>Child | £32.00</p>\\n<p>Senior | £34.00</p>\\n<p>Season ticket holders are entitled to a 5% <a class=\\"pxInta\\" href=\\"http://www.eastonfarmpark.co.uk/prices/#\\" id=\\"PXLINK_4_0_3\\" rel=\\"nofollow\\">discount</a> on purchases made in our Barmy Barn and <a class=\\"pxInta\\" href=\\"http://www.eastonfarmpark.co.uk/prices/#\\" id=\\"PXLINK_6_0_5\\" rel=\\"nofollow\\">Shop</a> and 11 children for the price of 10 on our birthday party packages.</p>\\n<p><strong>Note:</strong> A child ticket is for children that are 2 years old to 16 years old.</p>\\n<p>Under 2s are free</p>\\n",
