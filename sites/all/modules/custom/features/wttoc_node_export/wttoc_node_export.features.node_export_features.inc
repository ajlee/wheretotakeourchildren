      \'field_prices_information\' => array(
        \'und\' => array(
          array(
            \'value\' => "<h3><strong>2D &amp; 3D Tickets</strong><br />\\r\\n<br />\\r\\nChild (Under 18’s)£6.30<br />\\r\\nAdult£7.30<br />\\r\\nSenior£6.30<br />\\r\\nStudent£6.30<br />\\r\\nFamily of Four£21.00<br />\\r\\nFamily Singles£6.30<br />\\r\\nPurple Privilege Card£6.30<br />\\r\\nKids Club£1.49<br />\\r\\nEarly Bird<br />\\r\\n£4.20<br />\\r\\nHoliday Kids Club<br />\\r\\n£2.49<br />\\r\\n<br />\\r\\n<strong>Child</strong>&nbsp;- Age 17 and below<br />\\r\\n<strong>Senior</strong>&nbsp;- Age 60 and above<br />\\r\\n<strong>Student</strong>&nbsp;– Upon production of any&nbsp;<a class=\\"pxInta\\" href=\\"http://eastcoastcinema.co.uk/#\\" id=\\"PXLINK_1_0_0\\">Photo</a>&nbsp;ID student card.<br />\\r\\n<strong>Family of Four</strong>&nbsp;- valid for 4 members of a family made up of 1 adult and 3 children or 2 adults and 2 children. (Additional family members charged at the standard child rate). Valid only for U, PG and 12a movies. Children aged 14 and under.<br />\\r\\n<strong>Family Singles</strong>&nbsp;- Per ticket price for groups of 3 and below, or additional tickets to the ‘Family of Four’. Valid only for U, PG and 12a movies and including at least one child aged 14 and under.<br />\\r\\n<strong>Purple Privilege Card&nbsp;</strong>- Available to Adults upon production of a ‘Purple Privilege Card’<br />\\r\\n<strong>Kids Club – Per person.</strong>&nbsp;Only applicable to Sat/Sun movies advertised as ‘Kids Club’<br />\\r\\n<strong>Early Bird&nbsp;</strong>– Per Person. Only applicable to movies that&nbsp;<a class=\\"pxInta\\" href=\\"http://eastcoastcinema.co.uk/#\\" id=\\"PXLINK_5_0_4\\">start</a>&nbsp;before 10:30am.<br />\\r\\n<strong>‘Holiday’ Kids Club</strong>&nbsp;– Per person. Only applicable school holiday movies advertised as ‘Holiday Kids Club.’<br />\\r\\n<strong>3D Movies&nbsp;</strong>– Require 3D glasses. Use your own or purchase from the box office for £1.00 each.</h3>\\r\\n",
            \'format\' => \'filtered_html\',
            \'safe_value\' => "<h3><strong>2D &amp; 3D Tickets</strong>\\n</h3><p>Child (Under 18’s)£6.30<br />\\nAdult£7.30<br />\\nSenior£6.30<br />\\nStudent£6.30<br />\\nFamily of Four£21.00<br />\\nFamily Singles£6.30<br />\\nPurple Privilege Card£6.30<br />\\nKids Club£1.49<br />\\nEarly Bird<br />\\n£4.20<br />\\nHoliday Kids Club<br />\\n£2.49</p>\\n<p><strong>Child</strong> - Age 17 and below<br /><strong>Senior</strong> - Age 60 and above<br /><strong>Student</strong> – Upon production of any <a class=\\"pxInta\\" href=\\"http://eastcoastcinema.co.uk/#\\" id=\\"PXLINK_1_0_0\\" rel=\\"nofollow\\">Photo</a> ID student card.<br /><strong>Family of Four</strong> - valid for 4 members of a family made up of 1 adult and 3 children or 2 adults and 2 children. (Additional family members charged at the standard child rate). Valid only for U, PG and 12a movies. Children aged 14 and under.<br /><strong>Family Singles</strong> - Per ticket price for groups of 3 and below, or additional tickets to the ‘Family of Four’. Valid only for U, PG and 12a movies and including at least one child aged 14 and under.<br /><strong>Purple Privilege Card </strong>- Available to Adults upon production of a ‘Purple Privilege Card’<br /><strong>Kids Club – Per person.</strong> Only applicable to Sat/Sun movies advertised as ‘Kids Club’<br /><strong>Early Bird </strong>– Per Person. Only applicable to movies that <a class=\\"pxInta\\" href=\\"http://eastcoastcinema.co.uk/#\\" id=\\"PXLINK_5_0_4\\" rel=\\"nofollow\\">start</a> before 10:30am.<br /><strong>‘Holiday’ Kids Club</strong> – Per person. Only applicable school holiday movies advertised as ‘Holiday Kids Club.’<br /><strong>3D Movies </strong>– Require 3D glasses. Use your own or purchase from the box office for £1.00 each.\\n</p>",
          ),
