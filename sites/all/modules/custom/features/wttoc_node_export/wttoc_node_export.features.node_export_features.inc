      \'field_more_information\' => array(
        \'und\' => array(
          array(
            \'value\' => "<p><strong>Guided Tours</strong><br />\\r\\nDaily tours of West Stow Anglo-Saxon&nbsp;Village are available;&nbsp;please&nbsp;call to confirm before travelling - 012801284 7287184 728718.<br />\\r\\n<br />\\r\\n<strong>Angles Café </strong><br />\\r\\nThe Angles Café at West Stow is open daily and serves Costa coffee.&nbsp;<br />\\r\\nCafé open&nbsp;daily 10am-4pm)<br />\\r\\nKeep hold of your car park ticket stub and receive 10% off&nbsp;in the café!&nbsp;<br />\\r\\nDogs are allowed in the café (though not in the Anglo-Saxon Village - with the<br />\\r\\nexception of Guide Dogs).</p>\\r\\n\\r\\n<div>\\r\\n<h2><strong>Car Parking at West Stow</strong></h2>\\r\\n\\r\\n<div>&nbsp;</div>\\r\\n\\r\\n<div>£1 for up to an hour</div>\\r\\n\\r\\n<div>£2 for between 1 hour and all day</div>\\r\\n\\r\\n<div>Yearly passes are available - free with a Family Heritage Ticket, £10 with an Adult Heritage Ticket or £15 without a Heritage Ticket</div>\\r\\n</div>\\r\\n",
            \'format\' => \'filtered_html\',
