      \'field_image_gallery\' => array(
        \'und\' => array(
          array(
            \'fid\' => NULL,
            \'uid\' => \'39\',
            \'filename\' => \'crab-215170_1920.jpg\',
            \'uri\' => \'public://crab-215170_1920.jpg\',
            \'filemime\' => \'image/jpeg\',
            \'filesize\' => \'435882\',
            \'status\' => \'1\',
            \'timestamp\' => \'1446831467\',
            \'uuid\' => \'791f74b3-3f90-40fd-9592-009cbf463ff2\',
            \'rdf_mapping\' => array(),
            \'alt\' => \'Photograph of crab on the sand.\',
            \'title\' => \'Walberswick Beach crabbing\',
            \'width\' => \'1920\',
            \'height\' => \'1285\',
            \'node_export_file_path\' => \'public://crab-215170_1920.jpg\',
          ),
