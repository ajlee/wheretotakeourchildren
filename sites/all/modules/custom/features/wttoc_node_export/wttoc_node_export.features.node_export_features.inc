        \'und\' => array(
          array(
            \'value\' => "<p>For price information click <a href=\\"http://www.abbeygatecinema.co.uk/info/prices/\\">here</a></p>\\r\\n",
            \'format\' => \'filtered_html\',
            \'safe_value\' => "<p>For price information click <a href=\\"http://www.abbeygatecinema.co.uk/info/prices/\\" rel=\\"nofollow\\">here</a></p>\\n",
