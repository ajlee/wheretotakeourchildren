            \'status\' => \'1\',
            \'timestamp\' => \'1446834579\',
            \'uuid\' => \'aca5834d-e4ff-4794-8883-f09b0202f44e\',
            \'rdf_mapping\' => array(),
            \'alt\' => \'Informational poster for the Big Garden Party in Ipswich\',
